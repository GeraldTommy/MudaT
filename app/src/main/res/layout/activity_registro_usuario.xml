<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.itla.mudat3.RegistroUsuario">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp">

        <TextView
            android:id="@+id/tvId"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/txtmonbreu"
            android:layout_alignBottom="@+id/txtmonbreu"
            android:layout_alignParentStart="true"
            android:text="Nombre"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="27dp" />

        <EditText
            android:id="@+id/txtid"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_marginStart="51dp"
            android:layout_toEndOf="@+id/textView4"
            android:ems="10"
            android:inputType="textPersonName"
            tools:layout_editor_absoluteX="85dp"
            tools:layout_editor_absoluteY="16dp" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/txtid"
            android:layout_alignBottom="@+id/txtid"
            android:layout_alignParentStart="true"
            android:text="Usuario" />

        <EditText
            android:id="@+id/txtmonbreu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/txtid"
            android:layout_below="@+id/txtid"
            android:ems="10"
            android:inputType="textPersonName" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/txttipo"
            android:layout_alignBottom="@+id/txttipo"
            android:layout_alignParentStart="true"
            android:text="Tipo" />

        <EditText
            android:id="@+id/txttipo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/txtmonbreu"
            android:layout_below="@+id/txtmonbreu"
            android:ems="10"
            android:inputType="textPersonName" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/txttipo"
            android:layout_marginTop="16dp"
            android:text="Identificacion" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/txtemail"
            android:layout_alignBottom="@+id/txtemail"
            android:layout_alignParentStart="true"
            android:text="Email" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/txtclave"
            android:layout_alignParentStart="true"
            android:text="Telefono" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/txtclave"
            android:layout_alignBottom="@+id/txtclave"
            android:layout_alignParentStart="true"
            android:text="Clave" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/txtestatus"
            android:layout_alignBottom="@+id/txtestatus"
            android:layout_alignParentStart="true"
            android:text="Estatus" />

        <EditText
            android:id="@+id/txtidentificacion"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/txttipo"
            android:layout_below="@+id/txttipo"
            android:ems="10"
            android:inputType="textPersonName" />

        <EditText
            android:id="@+id/txtemail"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/txtidentificacion"
            android:layout_below="@+id/txtidentificacion"
            android:ems="10"
            android:inputType="textPersonName" />

        <EditText
            android:id="@+id/txttelefono"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/txtemail"
            android:layout_below="@+id/txtemail"
            android:ems="10"
            android:inputType="textPersonName" />

        <EditText
            android:id="@+id/txtclave"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/txttelefono"
            android:layout_below="@+id/txttelefono"
            android:ems="10"
            android:inputType="textPersonName" />

        <EditText
            android:id="@+id/txtestatus"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/txtclave"
            android:layout_below="@+id/txtclave"
            android:ems="10"
            android:inputType="textPersonName" />

        <Button
            android:id="@+id/btnacectar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/txtestatus"
            android:layout_marginTop="42dp"
            android:layout_toEndOf="@+id/tvId"
            android:text="Ejecutar" />

        <Button
            android:id="@+id/btncanselar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/txtestatus"
            android:layout_alignTop="@+id/btnacectar"
            android:layout_marginEnd="27dp"
            android:text="Canselar" />

    </RelativeLayout>

</android.support.constraint.ConstraintLayout>
